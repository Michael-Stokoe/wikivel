{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "algolia/algoliasearch-client-php": "^2.4",
        "artisanry/commentable": "^4.3",
        "askedio/laravel-soft-cascade": "^6.0",
        "erusev/parsedown-extra": "^0.7.1",
        "fideloper/proxy": "^4.0",
        "laravel/framework": "^6.0",
        "laravel/telescope": "^2.1",
        "laravel/tinker": "^1.0",
        "musonza/chat": "^3.5",
        "rtconner/laravel-likeable": "^2.0",
        "spatie/laravel-activitylog": "^3.8",
        "spatie/laravel-backup": "^6.4",
        "spatie/laravel-permission": "^3.0",
        "spatie/laravel-searchable": "^1.4",
        "spatie/laravel-sluggable": "^2.2",
        "spatie/laravel-tags": "^2.5"
    },
    "require-dev": {
        "league/html-to-markdown": "^4.8",
        "barryvdh/laravel-debugbar": "^3.2",
        "barryvdh/laravel-ide-helper": "^2.6",
        "facade/ignition": "^1.4",
        "fzaninotto/faker": "^1.4",
        "laravel-frontend-presets/tailwindcss": "^2.0",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.0",
        "spatie/laravel-web-tinker": "^1.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
